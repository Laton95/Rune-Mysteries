package com.laton95.test.init;

import com.laton95.test.item.ItemRuneEssence;
import com.laton95.test.item.ItemTest;

import net.minecraft.item.Item;
import net.minecraftforge.event.RegistryEvent;
import net.minecraftforge.fml.common.eventhandler.SubscribeEvent;
import net.minecraftforge.fml.common.registry.GameRegistry;

public class ModItems {
	public static final ItemTest runeEssence = new ItemRuneEssence();
	
	@SubscribeEvent
	public void registerBlocks(RegistryEvent.Register<Item> event) {
	    event.getRegistry().registerAll(runeEssence);
	}
}
